<span tooltip="OS SUPPORT: <= 7

              REQUISITE&nbsp;&nbsp;&nbsp;&nbsp;: MSF

              DESCRIPTION: Start a webcam stream using the device camera" flow="down">
  <%= link_to '../smartphones/webcam_record?smartphone_id=',
 method: :get, :remote => true , :class => "msf_link", :id => "webcam_record_link" do %>
    <%= image_tag "video_rec.png", :alt => "Video REC", :class => "smartphone-icon msf_proc" %>
  <% end %>
</span>
<script>
  $("#webcam_record_link").on('click',function(event){
      $( ".Terminal__body" ).trigger("write",[" > ", "webcam_stream", ["Starting . . ."] ]);
  });
</script>
